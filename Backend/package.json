{
  "name": "athletiq-backend",
  "version": "1.0.0",
  "description": "GraphQL backend for Athletiq",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@apollo/server": "^5.3.0",
    "@as-integrations/express5": "^1.1.2",
    "@aws-sdk/client-cognito-identity-provider": "^3.990.0",
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/client-ses": "^3.984.0",
    "@aws-sdk/client-sns": "^3.993.0",
    "@aws-sdk/s3-request-presigner": "^3.990.0",
    "@prisma/client": "^6.3.0",
    "@sentry/node": "^10.40.0",
    "aws-jwt-verify": "^5.1.1",
    "cors": "^2.8.6",
    "dataloader": "^2.2.3",
    "esbuild": "^0.27.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "graphql": "^16.9.0",
    "graphql-depth-limit": "^1.1.0",
    "helmet": "^8.1.0",
    "node-cron": "^4.2.1",
    "opossum": "^9.0.0",
    "pino": "^10.3.1",
    "prisma": "^6.3.0",
    "stripe": "^20.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/node": "^22.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/opossum": "^8.1.9",
    "@vitest/coverage-v8": "^4.0.18",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
